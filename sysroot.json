{
  "linux#qt5": {
    "source": "qt-everywhere-src-5.12.2.tar.xz",
    "edition": "opensource",
    "configure_options": ["-no-dbus"],
    "skip": ["qt3d", "qtwebengine"]
  },
  "linux#python": {
    "build_host_from_source": false,
    "build_target_from_source": true,
    "source": "Python-3.7.2.tgz",
