  "linux#sip": {
    "module_name": "PyQt5.sip",
    "source": "sip-4.19.15.tar.gz"
  },
  "linux#pyqt5": {
    "modules": ["QtCore", "QtGui", "QtWidgets"],
    "source": "PyQt5_*-5.12.2.tar.gz"
  }
}
